{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "ignore": ["node_modules"],
  "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-transform-modules-commonjs", "@babel/plugin-transform-runtime"]
}
