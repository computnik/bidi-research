{
  "name": "express-react-ts-mui-boilerplate",
  "version": "0.0.1",
  "description": "Computnik's full stack boilerplate",
  "main": "index.js",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "webpack --mode production",
    "build": "concurrently 'npm:build:server' 'npm:build:client'",
    "start": "npm run build && node .build",
    "dev:client": "webpack serve --mode development --devtool inline-source-map --hot",
    "dev:server": "nodemon",
    "watch:client": "webpack watch --mode development --devtool inline-source-map",
    "watch": "concurrently 'npm:dev:server' 'npm:watch:client'",
    "dev": "concurrently 'npm:dev:server' 'npm:dev:client'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:computnik/express-react-ts-mui-boilerplate.git"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "node",
    "express",
    "material-ui"
  ],
  "author": "Nikhil Motiani <motianinikhil@gmail.com>",
  "license": "MIT",
